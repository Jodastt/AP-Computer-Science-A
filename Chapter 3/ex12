class ex12 {
  public static void main(String[] args) {

    horizToVertical("Joseph");

  }

  public static void horizToVertical(String horizontal){

    System.out.println("");
    String[] stringArray = horizontal.split("");
    for(int i = 0; i < horizontal.length(); i++){

      System.out.println(stringArray[i]);

    }

  }
}
